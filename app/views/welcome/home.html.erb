
<div class="jumbotron">
  <h1>gCamp</h1>
  <p>Your life, organized.</p>
</div>

<div class="row">
  <div class="col-sm-4">
    <%=image_tag("mao1.jpg", class: "img-responsive")%></br>
  </div>

  <div class="col-sm-4">
    <%=image_tag("mao2.jpg", class: "img-responsive")%></br>
  </div>

  <div class="col-sm-4">
    <%=image_tag("mao3.jpg", class: "img-responsive")%></br>
  </div>
</div>

<div class="row">
  <div class="col-sm-4">
    <div class="panel panel-primary">
      <div class="panel-heading">
        <h3 class="panel-title">Tasks</h3>
      </div>
      <div class="panel-body">
        Grouped by projects and lists.  Just the way you like them.
      </div>
    </div>
  </div>
  <div class="col-sm-4">
    <div class="panel panel-primary">
      <div class="panel-heading">
        <h3 class="panel-title">Documents</h3>
      </div>
      <div class="panel-body">
        <ul>
          <li>Upload</li>
          <li>Comment</li>
          <li>Revise</li>
        </ul>
      </div>
    </div>
  </div>
  <div class="col-sm-4">
    <div class="panel panel-primary">
      <div class="panel-heading">
        <h3 class="panel-title">Comments</h3>
      </div>
      <div class="panel-body">
        <ul>
          <li>Comment on tasks and documents</li>
          <li>Get email notifications</li>
        </ul>
      </div>
    </div>
  </div>
</div>

<% @quotes.each do |sub_quote| %>
<blockquote>
  <p><%= sub_quote[0] %></p>
  <footer><i><%= sub_quote[1] %></i></footer>
</blockquote>
